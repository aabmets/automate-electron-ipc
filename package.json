{
   "name": "vite-plugin-electron-auto-ipc",
   "version": "0.0.0",
   "description": "A Vite plugin for Electron apps that auto-generates IPC bindings into the source files of the main process and the preload environment.",
   "keywords": ["vite", "plugin", "electron", "auto", "ipc"],
   "type": "module",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/aabmets/vite-plugin-electron-auto-ipc.git"
   },
   "author": "Mattias Aabmets <mattias.aabmets@gmail.com>",
   "types": "./types/index.d.ts",
   "module": "./dist/index.js",
   "exports": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
   },
   "files": ["dist", "types", "README.md", "LICENSE"],
   "scripts": {
      "check": "tsc -p tsconfig.json --noEmit && biome check",
      "pubtest": "tsc -p tsconfig.json && bun pm pack --destination ./packages",
      "pubnow": "biome check --error-on-warnings && tsc -p tsconfig.json && bun publish"
   },
   "devDependencies": {
      "@biomejs/biome": "^1.9.4",
      "@types/bun": "^1.1.12",
      "@types/node": "^22.8.2",
      "lefthook": "^1.8.1",
      "ts-node": "^10.9.2",
      "typescript": "^5.6.3",
      "vite": "^5.4.10",
      "vitest": "^2.1.4"
   },
   "peerDependencies": {
      "vite": "^5.4.10",
      "typescript": "^5.6.3"
   },
   "peerDependenciesMeta": {
      "vite": {
         "optional": true
      }
   }
}
